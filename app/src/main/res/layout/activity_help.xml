        android:layout_marginTop="5dp"
        />

    <TextView
        android:id="@+id/dictionary_text5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
